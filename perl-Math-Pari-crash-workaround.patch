 	     /* Now fall through: */
 /* case (IV)GENfirstOnStack: */
 	     /* Now sv is the newest one on stack. */
+	     if (howmany >= 0) {
 	     onStack_dec;
 	     perlavma = oldavma;
 	     if (oldavma > sentinel) {
@@ -3889,7 +3897,9 @@
